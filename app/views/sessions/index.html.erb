

<% if !@current_user %>
<div class="container">
    <%= link_to 'Create User', new_user_path %>
    <%= link_to 'Login', new_login_path %>
</div>
<% else %>

    <p><%= link_to "Start New Game", new_game_path, :class => "button btn btn-warning" %> </p>
    <p><%= link_to 'Logout', logout_path, method: :delete, :class => "button btn btn-warning" %></p>

<% end%>




